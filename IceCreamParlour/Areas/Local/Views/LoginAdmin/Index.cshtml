
@{
    Layout = null;
}

<div class="row justify-content-md-start">
    <div class="col-md-4">
        <div class="card">
            <div class="card-header">
                Register Success
            </div>
            <div class="card-body">
                @if (Session["Name"] != null)
                {
                    <p>Name: @Session["Name"].ToString()</p>
                    <p>Email: @Session["Email"].ToString()</p>
                    <a href="@Url.Action("Logout","LoginAdmin")" class="btn btn-warning">Back to Login</a>
                }
            </div>
        </div>

    </div>
</div>
